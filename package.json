{
  "name": "mini-fronted",
  "version": "0.1.0",
  "scripts": {
    "start": "npm-run-all --parallel start:*",
    "start:major": "cd major && yarn start:major",
    "start:minor-react": "cd minor-react && yarn start",
    "install": "npm-run-all --parallel install:*",
    "install:major": "cd major && yarn",
    "install:minor-react": "cd minor-react && yarn",
    "install:minor-umi": "cd minor-umi && yarn",
    "install:minor-vue": "cd minor-vue && yarn"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
